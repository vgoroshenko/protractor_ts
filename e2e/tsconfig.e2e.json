{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/e2e",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2018",
    "inlineSourceMap": true,
    "declaration": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "removeComments": true,
    "sourceMap": false,
    "types": [
      "jasmine",
      "jasminewd2",
      "node"
    ]
  }
}
